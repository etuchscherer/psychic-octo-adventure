.hero-unit.embossed
  %h1
    About me
    %br
    %small find out more

%h3.page-header
  my personal story

.row
  %dl.dl
    %span.span2.image-heading
      %dt I was born in Louisville, Ky.
      %dd
      =image_tag('carousel/kentucky.jpg')
    %span.span2.image-heading
      %dt and raised by my grandparents in New Jersey.
      %dd
      =image_tag('carousel/grandparents.jpg')
    %span.span2.image-heading
      %dt Looking south from the Empire State Building
      %dd
      =image_tag('carousel/newyork.jpg')
    %span.span2.image-heading
      %dt At 20, I joined the navy, and specialized in avionics.
      %dd
      =image_tag('carousel/qatar.jpg')
    %span.span2.image-heading
      %dt It gave me the opportunity to travel the world.
      %dd
      =image_tag('carousel/songbukdong.jpg')
    %span.span2.image-heading
      %dt I fell in love with Washington, and decided to settle here.
      %dd
      =image_tag('carousel/ranier.jpg')

.row
  %dl.dl
    %span.span2.image-heading
      %dt My family loves Washington also.
      %dd
      =image_tag 'carousel/oregon.jpg'
    %span.span2.image-heading
      %dt In my spare time, I enjoy building things
      %dd
      =image_tag 'carousel/building.jpg'
    %span.span2.image-heading
      %dt and hiking in the summertime
      %dd
      =image_tag 'carousel/hiking.jpg'
    %span.span2.image-heading
      %dt I go snowboarding at every available opportunity
      %dd
      =image_tag 'carousel/winter.jpg'
    %span.span2.image-heading
      %dt I helped found feral motion
      %dd
      =image_tag 'carousel/feralmotion.jpg'
    %span.span2.image-heading
      %dt  and was the first employee hired on at Salad Labs.
      %dd
      =image_tag 'carousel/working.jpg'

